pipeline {
  agent any
  environment {
    SCANNER_HOME= tool 'sonar'
  }
  stages {
    stage('Static Code Analysis') {
      steps {
        sh '''
        echo "${SCANNER_HOME}"
        '''
        }
      }
      stage('Unit Test') {
         steps {

                 echo 'Unit testing'
                 sh 'pip install -r ./requirements.txt'
         }
      }
    stage('build') {
      steps { 
             echo 'echo3'
      }
    } 
  }
}
