pipeline {
  agent any
  environment {
    SCANNER_HOME= tool 'sonar'
  }
  stages {
    stage('Static Code Analysis') {
      steps {
        sh '''
        echo "${SCANNER_HOME}"
        '''
        }
      }
      stage('Unit Test') {
         steps {
           sh 'test.py'
                 echo 'Unit testing'
         }
      }
    stage('build') {
      steps { 
             echo 'echo3'
      }
    } 
  }
}
