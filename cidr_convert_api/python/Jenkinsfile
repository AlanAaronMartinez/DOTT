pipeline {
  agent any
  environment {
    SCANNER_HOME= tool 'sonar'
  }
  stages {
    stage('Static Code Analysis') {
      steps {
        sh '''
        echo "${SCANNER_HOME}"
        '''
        withSonarQubeEnv('SonarCloud') {
        sh 'cd ./cidr_convert_api/python/'
        sh 'ls'
        sh 'pwd'
        sh '${SCANNER_HOME}/bin/sonar-scanner'
            }
        }
      }
      stage('Unit Test') {
         steps {
                 echo 'Unit testing'
         }
      }
    stage('build') {
      steps { 
             echo 'echo3'
      }
    } 
  }
}
