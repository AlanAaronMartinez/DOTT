pipeline {
  agent any
  tools {
    py 'python1.3'
  }
  environment {
    SCANNER_HOME= tool 'sonar'
  }
  stages {
    stage('Static Code Analysis') {
      steps {
        sh '''
        echo "${SCANNER_HOME}"
        '''
        }
      }
      stage('Unit Test') {
         steps {

                 echo 'Unit testing'
             sh label: 'script', script: 'python api.py'
         }
      }
    stage('build') {
      steps { 
             echo 'echo3'
      }
    } 
  }
}
